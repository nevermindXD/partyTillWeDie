    "bluebird": "^3.7.2",
    "body-parser": "^1.18.2",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "crypto-promise": "^2.1.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "*",
    "express-flash": "*",
    "express-session": "*",
    "express-validator": "^5.3.0",
    "helmet": "^3.22.0",
    "lusca": "^1.5.2",
    "moment": "^2.23.0",
    "mongoose": "^4.10.6",
    "newrelic": "^4.11.0",
    "nodemon": "^2.0.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sanitizer": "^0.1.3",
    "shelljs": "^0.8.3",
    "uuid": "^3.3.2",
    "wait-on": "^3.2.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
